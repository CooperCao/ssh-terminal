{
	"name": "ssh-bash",
	"displayName": "ssh-bash",
	"description": "ssh bash",
	"publisher": "cooper",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ssh-bash.open",
		"onCommand:ssh-bash.run",
		"onCommand:ssh-bash.close",
		"workspaceContains:.vscode/ssh.json"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ssh-bash.open",
				"title": "Open SSH-Bash"
			},
			{
				"command": "ssh-bash.run",
				"title": "Run on SSH-Bash"
			},
			{
				"command": "ssh-bash.close",
				"title": "Close SSH-Bash"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^5.1.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"async": "^2.6.0",
		"fs-extra": "^5.0.0",
		"ftp": "^0.3.10",
		"ignore": "^5.0.2",
		"joi": "^10.6.0",
		"lodash.debounce": "^4.0.8",
		"lru-cache": "^4.1.3",
		"p-queue": "2.4.2",
		"ssh-config": "^1.1.3",
		"ssh2": "^0.8.5",
		"tmp": "^0.0.33",
		"upath": "^1.0.0"
	}
}
